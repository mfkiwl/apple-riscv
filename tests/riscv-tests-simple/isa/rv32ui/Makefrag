rv32ui_tests_name = \
	simple \
	xor xori \
	or ori \
	and andi \
	add addi \
	add addi \
	sll slli \
	srl srli \
	sra srai \
	sub 	 \
	lb lbu	 \
	lh lhu	 \
	lw	     \
	sb sh sw \
	lui		 \
	auipc	 \
	jal jalr \
	beq bne  \
	bge bgeu \
	blt bltu \
	slt slti \
	sltu sltiu \

rv32ui_tests = $(addprefix rv32ui-, $(rv32ui_tests_name))

